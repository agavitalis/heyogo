	 {{> header}}
	<!-- ck title wrapper -->
	<div class="ck_title_main_wrapper float_left">
		<div class="container">
			<div class="row">
				<div class="col-xl-6 col-lg-6 col-md-6 col-sm-6 col-12">
					<div class="ck_title_cont float_left">
						<p>Products</p>
					</div>
				</div>
				<div class="col-xl-6 col-lg-6 col-md-6 col-sm-6 col-12">
					<div class="ck_title_cont float_left">
						<ul>
							<li><a href="/">Home &nbsp; ></a>
							</li>
							<li>Products</li>
						</ul>
					</div>
				</div>
			</div>
		</div>
	</div>
	<!-- ck title wrapper End -->
	<!-- btc Shop Wrapper Start -->
	<div class="btc_shop_indx_main_wrapper">
		<div class="container">
			<div class="row">
				<div class="col-md-12">
					<div class="ck_prod_filter_main_box_wrapper">
						<div class="row">
							<div class="col-lg-5 col-md-5 col-sm-12 col-12">
								<div class="gc_shop_indx_search_box_wrapper">
									<div class="gc_shop_indx_search_box">
										<p><span>Our Products</span></p>
									</div>
								</div>
							</div>
							<div class="col-lg-3 col-md-3 col-sm-12 col-12">
								<div class="gc_shop_indx_view_tabs_wrapper">
									<div class="gc_shop_indx_view_tabs">
										<ul class="nav nav-tabs">
											<li class="nav-item">
												<a class="nav-link active" data-toggle="tab" href="#menu1"> <i class="flaticon-menu"></i>
												</a>
											</li>
											<li class="nav-item"> <a class="nav-link" data-toggle="tab" href="#menu2"><i class="flaticon-list"></i> </a>
											</li>
										</ul>
									</div>
								</div>
							</div>
							<div class="col-lg-4 col-md-4 col-sm-12 col-12">
								<div class="gc_shop_indx_select_box_wrapper">
									<div class="gc_causes_select_box">
										<select>
											<option>Sort Default</option>
											
										</select><i class="fa fa-angle-down"></i>
									</div>
								</div>
							</div>
						</div>
					</div>
					<div class="row">
						<div class="col-lg-12 col-md-12 col-sm-12 col-12">
							<div class="tab-content">
								<div id="menu1" class="tab-pane active">
									<div class="row">
										{{#each products}}
											
											<div class="col-lg-4 col-md-6 col-sm-6 col-12">
												<div class="ck_elem_slide_img_wrapper ck_elem_slide_img_wrapper_inner float_left">
													<div class="ck_elem_slide_img float_left">
														<img src="uploads/{{picture}}" alt="img">
													</div>
													<div class="ck_elem_slide_img_cont ck_elem_slide_img_cont_inner float_left">
														<h3>{{product_name}}</h3>
														<div class="ck_elem_star">	<i class="fas fa-star"></i>
															<i class="fas fa-star"></i>
															<i class="fas fa-star"></i>
															<i class="fas fa-star"></i>
															<i class="fas fa-star"></i>
														</div>
														<p>{{product_description}}</p>
														{{!-- <h5>$ 10.00</h5>
														<div class="ck_offer_btn ck_offer_btn_inner float_left">	<a href="#">Add to Cart</a>
														</div> --}}
													</div>
												</div>
											</div>


										{{/each}}
										
									
										
										<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 hidden-sm hidden-xs">
											<div class="pager_wrapper">
												<ul class="pagination">
													<li><a href="#"><i class="flaticon-left-arrow"></i></a>
													</li>
													<li class="btc_shop_pagi"><a href="#">01</a>
													</li>
													<li class="btc_shop_pagi"><a href="#">02</a>
													</li>
													<li class="btc_third_pegi btc_shop_pagi"><a href="#">03</a>
													</li>
													<li class="hidden-xs btc_shop_pagi"><a href="#">04</a>
													</li>
													<li><a href="#"><i class="flaticon-right-arrow"></i></a>
													</li>
												</ul>
											</div>
										</div>
									</div>
								</div>
								<div id="menu2" class="tab-pane fade">
									<div class="row">
										{{#each products}}
											<div class="col-md-12">
												<div class="ck_elem_slide_img_wrapper ck_elem_slide_img_wrapper_inner ck_elem_slide_img_wrapper_inner_list float_left">
													<div class="ck_elem_slide_img ck_elem_slide_img_list float_left align-self-center">
														<img src="uploads/{{picture}}" alt="img">
													</div>
													<div class="ck_elem_slide_img_cont ck_elem_slide_img_cont_inner ck_elem_slide_img_list_cont float_left">
														<h3>{{product_name}}</h3>
														<div class="ck_elem_star">	<i class="fas fa-star"></i>
															<i class="fas fa-star"></i>
															<i class="fas fa-star"></i>
															<i class="fas fa-star"></i>
															<i class="fas fa-star"></i>
														</div>
														<p>{{product_description}}</p>
														{{!-- <h5>$ 10.00</h5>
														<div class="ck_offer_btn ck_offer_btn_inner float_left">	<a href="#">Add to Cart</a>
														</div> --}}
													</div>
												</div>
											</div>
										{{/each}}


					
										
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<!-- btc Shop Wrapper End -->
