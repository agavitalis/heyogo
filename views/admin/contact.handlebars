{{> header}}
<!-- ck title wrapper -->
<div class="ck_title_main_wrapper float_left">
    <div class="container">
        <div class="row">
            <div class="col-xl-6 col-lg-6 col-md-6 col-sm-6 col-12">
                <div class="ck_title_cont float_left">
                    <p>Admin</p>
                </div>
            </div>
            <div class="col-xl-6 col-lg-6 col-md-6 col-sm-6 col-12">
                <div class="ck_title_cont float_left">
                    <ul>
                        <li><a href="#">Admin &nbsp; ></a>
                        </li>
                        <li>Contacts</li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- ck title wrapper End -->


<!-- btc Shop Wrapper Start -->
<div class="btc_shop_indx_main_wrapper">
    <div class="container">
        <div class="row">
           {{> admin_sidebar}}
            <div class="col-xl-9 col-lg-9 col-md-12 col-sm-12 col-12">
                <div class="ck_prod_filter_main_box_wrapper">
                    <div class="row">

                        <div class="col-lg-4 col-md-6 col-sm-6">
                            <div class="gc_shop_indx_select_box_wrapper">
                                <div class="gc_causes_select_box">
                                    <h4>Messages for Admin</h4>
                                </div>
                            </div>
                        </div>
                       
                    </div>
                </div>
                <div class="row">
                    <div class="column col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <table class="table table-bordered">
                            <thead>
                                <tr>
                                    <th scope="col">#</th>
                                    <th scope="col">Name</th>
                                    <th scope="col">Email</th>
                                    <th scope="col">Message</th>
                                </tr>
                            </thead>
                            <tbody>
                               
                                {{#each messages}}
                                    <tr>
                                        <th scope="row">1</th>
                                        <td>{{name}}</td>
                                        <td>{{email}}</td>
                                        <td>{{message}}</td>
                                        	
                                    </tr>
                                {{/each}}
                              
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>
<!-- btc Shop Wrapper End -->

{{> footer}}