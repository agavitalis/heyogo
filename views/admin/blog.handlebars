<!-- btc Shop Wrapper Start -->
<div class="btc_shop_indx_main_wrapper">
    <div class="container">
        <div class="row">
            <div class="col-xl-3 col-lg-3 col-md-12 col-sm-12 col-12 d-lg-block d-xl-block">

                <div class="cc_pc_first_accordion_wrapper cc_pc_second_accordion_wrapper">
                    <div class="cc_pc_accordion">
                        <ul id="accordion2" class="accordion">
                            <li class="default open">
                                <div class="link cc_product_heading">Menu<i class="fa fa-chevron-down"></i>
                                </div>
                                <ul class="submenu">
                                    <li>
                                        <div class="content">
                                            <div class="box">
                                                <p class="cc_pc_color1">
                                                    <input type="checkbox" id="c31" name="cb">
                                                    <label for="c31">Products</label>
                                                </p>
                                                <p class="cc_pc_color2">
                                                    <input type="checkbox" id="c32" name="cb">
                                                    <label for="c32">Blog</label>
                                                </p>
                                                <p class="cc_pc_color3">
                                                    <input type="checkbox" id="c33" name="cb">
                                                    <label for="c33">Gallery</label>
                                                </p>
                                                <p class="cc_pc_color4">
                                                    <input type="checkbox" id="c34" name="cb">
                                                    <label for="c34">Users</label>
                                                </p>

                                            </div>
                                        </div>
                                    </li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                </div>


            </div>
            <div class="col-xl-9 col-lg-9 col-md-12 col-sm-12 col-12">
                <div class="ck_prod_filter_main_box_wrapper">
                    <div class="row">

                        <div class="col-lg-2 col-md-4 col-sm-3">
                            <div class="gc_shop_indx_select_box_wrapper">
                                <div class="gc_causes_select_box">
                                    <h4>Posts</h4>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-10 col-md-8 col-sm-9">
                            <div id="accordion">

                                <button class="btn btn-success float-right mb-5" data-toggle="collapse"
                                    data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                    Add Post
                                </button>

                                <div id="collapseOne" class="collapse" aria-labelledby="headingOne"
                                    data-parent="#accordion">
                                    <div class="card-body">
                                        <!--Product Details-->
                                        <div class="checkout-form">
                                            <div class="row clearfix">
                                                <!--Column-->
                                                <div class="column col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                    <div
                                                        class="btc_shop_single_prod_right_section related_pdt_shop_head checkout_heading">
                                                        <h1>Post Details </h1>
                                                    </div>
                                                    <div class="row clearfix">
                                                        <!--Form Group-->
                                                        <div class="form-group col-md-12 col-sm-12 col-xs-12">
                                                            <div class="field-label">Post Title <sup>*</sup>
                                                            </div>
                                                            <input type="text" name="field-name" value=""
                                                                placeholder="">
                                                        </div>

                                                        <!--Form Group-->
                                                        <div class="form-group col-md-12 col-sm-12 col-xs-12">
                                                            <div class="field-label">Post Body</div>
                                                            <textarea></textarea>
                                                        </div>
                                                        <!--Form Group-->
                                                        <div class="form-group col-md-12 col-sm-12 col-xs-12">
                                                            <div class="field-label">Description Image</div>
                                                            <input type="file" name="field-name" value=""
                                                                placeholder="">
                                                        </div>
                                                        <div class="form-group col-md-12 col-sm-12 col-xs-12">
                                                            <button class="btn btn-success float-right"> Add
                                                                Post</button>

                                                        </div>

                                                    </div>
                                                </div>



                                            </div>
                                        </div>
                                        <!--End Product Details-->
                                    </div>
                                </div>

                            </div>
                        </div>

                    </div>
                </div>
                <div class="row">
                    <div class="column col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <table class="table table-bordered">
                            <thead>
                                <tr>
                                    <th scope="col">#</th>
                                    <th scope="col">First</th>
                                    <th scope="col">Last</th>
                                    <th scope="col">Handle</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <th scope="row">1</th>
                                    <td>Mark</td>
                                    <td>Otto</td>
                                    <td>@mdo</td>
                                </tr>
                                <tr>
                                    <th scope="row">2</th>
                                    <td>Jacob</td>
                                    <td>Thornton</td>
                                    <td>@fat</td>
                                </tr>
                               
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>
<!-- btc Shop Wrapper End -->

{{> footer}}